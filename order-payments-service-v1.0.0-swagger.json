          },
          "accountId" : {
            "type" : "string",
            "description" : "The identifier of the account. Often referred to as the account number, yet for consistency this is always referred to as accountId.",
            "maxLength" : 16
          },
          "displayName" : {
            "type" : "string",
            "description" : "The name used for display purposes",
            "maxLength" : 35
          },
          "currencyId" : {
            "type" : "string",
            "description" : "The identifier of the curreny. The is the 3 letter ISO 4217 code of the currency.",
            "maxLength" : 3
          },
          "lockedAmount" : {
            "type" : "number",
            "format" : "int64",
            "description" : "The amount of funds that have been blocked (reserved) for specific purposes"
          },
          "workingBalance" : {
            "type" : "number",
            "format" : "int64",
            "description" : "For Nostro and Internal Accounts, the same as Online Cleared Balance. For all other accounts it is the cleared balance plus the total of all unapproved debits."
          },
          "onlineActualBalance" : {
            "type" : "number",
            "format" : "int64",
            "description" : "Balance of the account that represents total of transactions posted against the account excluding any entries with the processing date in the future."
          },
          "availableBalance" : {
            "type" : "number",
            "format" : "int64",
            "description" : "Projected net movements of funds against the account. Depending on the setup in the field AVAILABLE.BAL.UPD in the ACCOUNT.PARAMETER record, may or may not include unauthorised debits, credits, both debits and credits."
          },
          "sortCode" : {
            "type" : "string",
            "description" : "The bank identifier code which identifies both the bank and the branch where the account is held"
          },
          "customerId" : {
            "type" : "string",
            "description" : "The identifier of the customer",
            "maxLength" : 10
          },
          "availableLimit" : {
            "type" : "string",
            "description" : "The amount of an allocated credit limit that is available for the nomniated customer to utilise",
            "maxLength" : 19
          },
          "IBAN" : {
            "type" : "string",
            "maxLength" : 19
          },
          "portfolioId" : {
            "type" : "string",
            "description" : "The identifier of the portfolio",
            "maxLength" : 18
          },
          "openingDate" : {
            "type" : "string",
            "format" : "date"
          },
          "limitReference" : {
            "type" : "string",
            "description" : "Identifies the type of limit applicable to the Account",
            "maxLength" : 10
          },
          "clearedBalance" : {
            "type" : "number",
            "format" : "int64",
            "description" : "Total of cleared funds booked against the account. Same as onlineActuaBalance except credit or reversed debit entries with the exposure date in the future."
          }
        }
      }
