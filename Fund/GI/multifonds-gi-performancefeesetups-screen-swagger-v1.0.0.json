ntInternalId":{"type":"string","description":"Internal identifier of the parent record"},"parentExternalId":{"type":"string","description":"External identifier of the parent record"},"parentPromoterInternalId":{"type":"string","description":"Internal identifier of the Fund promoter linked to the parent entity"},"parentPromoterExternalId":{"type":"string","description":"External identifier of the Fund promoter linked to the parent entity"},"performancefeesetup":{"type":"string","description":"Contains individual attributes of the Performance fee setup entity"},"shareClassCode":{"type":"string","description":"Shareclass code for which the performance fee is calculated. Allowed values are from the list of shareclass codes defined in MFGI."},"hwmApplicableFlag":{"type":"string","description":"Flag allows to enable HWM (High Water Mark) for performance fee calculation. Allowed values: Y(for Yes), N(for No). "},"hwmPrinciple":{"type":"string","description":"It specifies the Principle to reset the HWM. The field is mandatory and updatable only if flag HWM applicable is set as Y; otherwise its not updatable. Possible values: 0001(Highest ever), 0002(Never reset), 0003(Highest over a period). The LoV is available in the CMESS table HWM_PRIN."},"hwmPeriodInMonths":{"type":"number","description":"It specifies the Observation period for the HWM reset in months. The field is mandatory if HWM Principle is set as 0003(Highest over a period), otherwise its not updatable. Maximum allowed value is 120. "},"rateType":{"type":"string","description":"Rate type code to calculate the performance fee. Possible values: 0001(Rate),  0002 (Scale). The LoV is available in the CMESS table RATE_TYPE."},"ratePercentage":{"type":"number","description":"Performance fee rate in percentage. The field is mandatory if Rate type  is set as 0001(Rate); otherwise its not updatable. If this rate is changed, the share details window will also reflect the new rate. It will happen only when the rate type is marked as 0001-Rate"},"scaleCode":{"type":"string","description":"The performance fee scale code type.The field is mandatory if Rate type is set as 0002(Scale); otherwise the field is not updatable. If a scale is defined, then the PF% at the share details window will not have any impact. Allowed values are from the list of scale codes defined in MFGI."},"dpForCrpPfAccrual":{"type":"string","description":"Decimal places for CRP/PF accrual. Possible Values: 0 (Integer), 1(1 Decimal- places), 2 (2 Decimal places) etc. The LoV is available in the CMESS table CODE_ARR."},"dpForReferenceNav":{"type":"string","description":"Decimal places for Reference NAV (HAHWM). Possible Values: 0 (Integer), 1(1 Decimal- places), 2 (2 Decimal places) etc. The LoV is available in the CMESS table CODE_ARR."},"dpForGrossNav":{"type":"string","description":"Decimal places for Gross NAV. Possible Values: 0 (Integer), 1(1 Decimal- places), 2 (2 Decimal places) etc. The LoV is available in the CMESS table CODE_ARR."},"hurdleApplicableFlag":{"type":"string","description":"Flag allows to enable hurdle for performance fee calculation. Allowed values: Y(for Yes), N(for No)."},"hurdleType":{"type":"string","description":"Type of hurdle. Possible values: 0001(Hard Hurdle), 0002(Soft hurdle). The LoV is available in the CMESS table HURDLE_TYP."},"catchUpPercentage":{"type":"number","description":"Catch-up (%)"},"hurdleMethod":{"type":"string","description":"Hurdle rate method. Possible values: 0001(Fixed rate), 0002(Benchmark variable rate). The LoV is available in the CMESS table HURDLE_RM."},"hurdleRate":{"type":"number","description":"Hurdle rate in Percentage. Hurdle rate is allowed only if Hurdle rate method is 0001(Fixed rate). If Hurdle rate method is 0002(Benchmark variable rate), the Hurdle rate will be updatable."},"dayCountMethod":{"type":"string","description":"Method used for days count. Possible values: 0001(ACT/ACT), 0005(ACT/denominator), 0007(BD/denominator). The LoV is available in the CMESS table DC_METD."},"denominator":{"type":"number","description":"Denominator for the number of days. The field is Mandatory if Days Count Methodis set as 0005 (ACT/denominator) or 0007(BD/denominator). If the Day count method equals 0001 (ACT/ACT) or 0006 (30/360), the Denominator field will not be updatable. "},"hurdleRateType":{"type":"string","description":"Hurdle rate method. Possible values: 0001(Flat rate), 0002(Compound), 0003(Reverse compound). The LoV is available in the CMESS table HURDLE_RT."},"compoundingBasis":{"type":"string","description":"It specifies the Basis for compounding. The field is mandatory if Hurdle rate type is 0002-(Compound). Possibles values: 0001(Each GAV), 0002(Each observation reset date). The LoV is available in the CMESS table COMP_BASIS."},"resetHurdle":{"type":"string","description":"The method to reset the hurdle. Possible values: 0001(HAHWM < Gross NAV) , 0002 (HAHWM>= Gross NAV or HWM), 0003 (No reset). The LoV is available in the CMESS table HURDLE_RS."},"benchmarkType":{"type":"string","description":"Benchmark type code. Possible values: 0001 (Security) , 0002 (Interest rate). The LoV is available in the CMESS table BMK_TYP."},"benchmarkSecurityId":{"type":"number","description":"Internal ID of the Bench Mark Security ID"},"hurdleRateOnTopOfBmk":{"type":"number","description":"Additional rate for Hurdle rate. The value can be negative or positive. The field will be active only if the Hurdle rate method is set as 0002(Benchmark variable rate. If the Hurdle rate method is set as 0001(Fixed rate), then the field will not be updatable."},"hurdleRateOnTopOfBmkType":{"type":"string","description":"It specifies if the Hurdle rate on top of the benchmark is to be considered as a percent value or a percentage to be added to the benchmark. The field is Mandatory if Hurdle Rate on top of the BMK is specified. Possible values: 0001 (Straight pct. addition), 0002 (Pct. multiplication).  The LoV is available in the CMESS table HURDLE_BMK."},"observationPeriod":{"type":"string","description":"The Observation period for the hurdle. The field is mandatory if the compounding basis 0002(Each observation reset date). For example: 0001(End of month), 0002(Quaterly), 0005(Monthly)etc. "},"observationStartDate":{"type":"string","format":"date","example":"dd-MM-yyyy","description":"The Observation start date. It is mandatory if the field Observation period is setup."},"observationNextTheoreticalDate":{"type":"string","format":"date","example":"dd-MM-yyyy","description":"Next theoretical observation date. The field is automatically populated by the system based on the observation period and the observation date. It should be greater than the observation date."},"observationDateType":{"type":"string","description":"Observation date Type"},"observationDateToBeUsed":{"type":"string","description":"Observation date to be used"},"observationCalendar":{"type":"string","description":"Observation calendar"},"observationDateForInception":{"type":"string","format":"date","example":"dd-MM-yyyy","description":"Observation date for inception"},"referenceNavNetDividendFlag":{"type":"string","description":"Reference NAV/HWM/HAHWM net dividend"},"benchmarkSecurityExternalIdType":{"type":"string","description":"External ID of the Bench Mark Security ID Type. "},"benchmarkSecurityExternalId":{"type":"string","description":"External ID of the Bench Mark Security ID"},"performanceFeeCode":{"type":"string","description":"Performance fee code"},"performanceFeeCalculationStatus":{"type":"string","description":"Performance fee calculation status"},"observationNextTheoreticalDateSystem":{"type":"string","format":"date","example":"dd-MM-yyyy","description":"Next theoretical date"}}},"postPerformancefeesetupsSuccessheaderResponse":{"properties":{"pagination":{"$ref":"#/definitions/paginationSchema"},"audit":{"$ref":"#/definitions/auditSchema"},"status":{"$ref":"#/definitions/successHeaderstatusSchema"},"company":{"type":"string"